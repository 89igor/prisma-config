generator client {
  provider = "prisma-client-js"
  output   = "./generated/game_club"
}

datasource db {
  provider = "mysql"
  url      = "mysql://root:Alibaba@1408@localhost:3306/game_club"
}

model Apply_club {
  id                        Int               @id                         @default(autoincrement())     
  club_id                   String 
  apply_user                Int 
  apply_user_name           String 
  apply_time                DateTime 
  apply_result              Int 
  apply_user_head_url       String 
}

model Club_list {
  id                        Int               @id                         @default(autoincrement())     
  club_name                 String 
  create_user               Int 
  content                   String                                      
  club_manage               Int                                      
  club_id                   String                                      
  user_num                  Int                                      
  create_user_name          String                                             
  create_user_head_url      String                                             
  club_manage_name          String                                             
  club_manage_head_url      String                                             
  table_info                String                                             
}

model Club_user_list {
  id                        Int               @id                         @default(autoincrement())     
  club_id                   String 
  user_id                   Int 
  user_name                 String 
  user_head_url             String 
}

model User_club_num_list {
  user_id                   Int               @id                         
  club_num                  Int 
}