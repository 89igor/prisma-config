  wincoinvalue              Int 
}

model Matchrandking { 
  id                        Int             @id                     @default(autoincrement())         
  roomType                  Int 
  matchId                   Int 
  userId                    Int        
  score                     Int 
  lastTime                  DateTime        @default(now()) 
  winPropId                 Int 
  winPropCount              Int 
  winScore                  Int 
  rankIdx                   Int 
  isGetPrize                Int 
  isMsg                     Int 
  title                     String 
  msg                       String 
}

model Pool {
  serveId                   Int             @id                         
  pool                      Int 
  virtualPool               Int 
}

model Sendprize {
  idx                       Int             @id                      @default(autoincrement())        
  propid                    Int 
  propcount                 Int 
  score                     Int 
}

model Shootprize {
  lv                        Int             @id                      @default(autoincrement())        
  value                     Int 
  propid                    Int 
  propcount                 Int 
