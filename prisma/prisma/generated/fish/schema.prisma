model T_users {
  userid                    Int             @id                      @default(autoincrement())        
  account                   String 
  name                      String         
  sex                       Int 
  headimg                   String 
  lv                        Int 
  exp                       Int 
  coins                     Int 
  vip                       Int 
  money                     Int 
  gems                      Int 
  roomid                    String 
  history                   String 
