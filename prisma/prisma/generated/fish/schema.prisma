generator client {
  provider = "prisma-client-js"
  output   = "./generated/fish"
}

datasource db {
  provider = "mysql"
  url      = "mysql://root:Alibaba@1408@localhost:3306/fish"
}

model Catch_chance {
  serveId                   Int             @id                         
  chance                    Float	        
}

model Control_pool {
  serveId                   Int             @id                         
  pool                      Int 
  line                      Int
}

model Control_user {
  uid                       Int             @id                         
  chance                    Float 
}

model Daysendprizevalue {
  day                       Int             @id                         
  value                     Int 
}

model Fishlog {
  id                        Int             @id                     @default(autoincrement())         
  userid                    Int 
  usecoin                   Int 
  wincoin                   Int 
  balanceTime               DateTime        @default(now())
  mark                      Int 
  serverId                  Int 
}

model Getcoin { 
  id                        Int             @id                     @default(autoincrement())         
  userId                    Int 
  getCoin                   Int 
  adddate                   DateTime        @default(now())
  mark                      Int 
  isget                     Int 
  day                       Int 
}

model Lv {
