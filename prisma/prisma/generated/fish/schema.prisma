}

model T_message {
  type                      String          @id                         
  msg                       String 
  version                   String         
}

model T_property {
  propId                    Int             @id                      @default(autoincrement())        
  userid                    Int 
  ice                       Int         
}

model T_rooms {
  uuid                      String          @id                         
  id                        String 
  base_info                 String         
  create_time               Int 
  num_of_turns              Int 
  next_button               Int 
