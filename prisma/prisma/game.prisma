generator client {
  provider = "prisma-client-js"
  output   = "./generated/game"
}

datasource db {
  provider = "mysql"
  url      = "mysql://root:Alibaba@1408@localhost:3306/game"
}

model T_accounts {
  account                   String            @id                         
  password                  String 
  reg_time                  Int 
}

model T_charge_log {
  id                        Int               @id                    @default(autoincrement())          
  orderno                   String 
  userid                    Int 
  gems_num                  Int                                      
  cost_money                Int                                      
  charge_type               String                                      
  time                      Int                                      
  goldcoin_exchange_rate    Float                                             
}

model T_game_result_log {
  id                        Int               @id                    @default(autoincrement())          
  roomid                    Int 
  tax                       Int 
  data                      String 
  time                      Int 
}

model T_games {
  room_uuid                 String            @id                         
  game_index                Int 
  base_info                 String 
  create_time               Int 
  snapshots                 String 
  action_records            String 
  result                    String 
}

model T_games_archive {
  room_uuid                 String            @id                         
  game_index                Int 
  base_info                 String         
  create_time               Int 
  snapshots                 String 
  action_records            String 
  result                    String 
}

model T_guests {
  guest_account             String            @id                 
}

model T_message {
  type                      String            @id                         
  msg                       String 
  version                   String         
}

model T_property {
  propId                    Int               @id                         
  userid                    Int 
  ice                       Int         
}

